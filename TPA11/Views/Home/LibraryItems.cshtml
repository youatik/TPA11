@model List<LibraryItem>  

<h1>Library Items</h1>

<table class="table">
    <thead>
        <tr>
            <th>EAN / ISBN</th>
            <th>Title</th>
            <th>Creators</th>
            <th>Description</th>
            <!-- Add more columns as needed -->
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ean_isbn13</td>
                <td>@item.Title</td>
                <td>@item.Creators</td>
                <td>@item.Description</td>
                <!-- Add more columns as needed -->
            </tr>
        }
    </tbody>
</table>
